Rxvt*font: -misc-fixed-medium-r-normal--15-140-75-75-c-90-iso10646-1 
XTerm*font: -misc-fixed-medium-r-normal--15-140-75-75-c-90-iso10646-1 

Rxvt*scrollBar:                False
Rxvt*internalBorder:           0
Rxvt*termName:                 rxvt-unicode-256color
Rxvt*urlLauncher:              midori -p
Rxvt*saveLines: 10000
Rxvt*jumpScroll: True
Rxvt*secondaryScroll: False

*background: #000000
*foreground: #CCCCCC
!*foreground: #FFCF00

