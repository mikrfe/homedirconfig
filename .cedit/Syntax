    keyword wholeleft yellow\/ yellow/yellow
    keyword wholeleft brightblue\/ brightblue/blue1
    keyword wholeleft brightmagenta\/ brightmagenta/magenta
    keyword wholeleft brightcyan\/ brightcyan/cyan1
